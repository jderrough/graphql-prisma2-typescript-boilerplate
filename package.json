{
  "name": "graphql-prisma2-typescript-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "@prisma/photon": "^0.2.94",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-ws": "^1.0.19",
    "apollo-utilities": "^1.3.2",
    "bcryptjs": "^2.4.3",
    "graphql": "^14.5.8",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "nexus": "0.12.0-beta.12",
    "nexus-prisma": "0.5.0-next.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/node": "^12.7.9",
    "@types/ws": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "cross-fetch": "^3.0.4",
    "env-cmd": "^10.0.1",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "prisma2": "2.0.0-preview-13.2",
    "ps-node": "^0.1.6",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^1.0.0-pre.43",
    "typescript": "3.5.3"
  },
  "scripts": {
    "postinstall": "yarn -s generate",
    "clean": "rm -rf dist",
    "build": "yarn -s clean && yarn -s generate && tsc",
    "style": "prettier --write 'src/**/*'",
    "start": "node dist/src/index.js",
    "heroku-postbuild": "rm -rf dist/ && tsc",
    "predev": "env-cmd -f ./config/dev.env yarn -s generate",
    "dev": "env-cmd -f ./config/dev.env ts-node-dev --no-notify --respawn --transpileOnly ./src",
    "generate": "yarn -s generate:prisma && yarn -s generate:nexus",
    "generate:prisma": "prisma2 generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "pretest": "prisma2 lift save --name \"init-test\" -c && prisma2 lift up",
    "pretest:local": "env-cmd -f ./config/test.env prisma2 lift save --name \"init-test\" -c && env-cmd -f ./config/test.env prisma2 lift up",
    "posttest": "rm -rf prisma/test.db prisma/migrations/*-init-test",
    "test": "npx jest --runInBand",
    "test:local": "env-cmd -f ./config/test.env npx jest --runInBand",
    "test:watch": "env-cmd -f ./config/test.env npx jest --runInBand --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
